function _update()
	--controls
	--right/left moves points
	--up/down moves entire slides
	if (btnp(‚û°Ô∏è)) then
		slidecounter+=1
		stayinloop = 1
		while stayinloop == 1 do
			if stringhasbullet[slidecounter + 1]
			 == 0 then
				slidecounter+=1
				if slidecounter >= 
					(#slidestrings + 1) then
					stayinloop = 0
				end
			else
				stayinloop = 0	
			end
		end	
		if slidecounter >= 
			(#slidestrings + 1) then
			slide+=1
			if slide > slidelimit then
				slide = 0
			end
			_slidechanged()
		end
	end
	if (btnp(‚¨ÖÔ∏è)) then
		slidecounter-=1
		if slidecounter < 0 then
			slide-=1
			if slide < 0 then
				slide = 0
			end
			_slidechanged()
		else
			bulletx[slidecounter+1] = 128
		end
	end
	if (btnp(‚¨ÜÔ∏è)) then
		slide+=1
		if slide > slidelimit then
			slide = 0
		end
		_slidechanged()
	end	
	if (btnp(‚¨áÔ∏è)) then
		slide-=1
		if slide < 0 then
			slide = 0
		end
		_slidechanged()
	end
	
	--example of a bypass button
	--to get to certain slide
	if (btn(‚ùé)) then
		slide = 0
		_slidechanged()
	end
	
	if(slide == 0 and slidecounter == 0) then
		update_logo()
	end
	--update particles
	--comment out to disable
 update_psystems()
end


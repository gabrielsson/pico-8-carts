 		end
 		if stringhasbullet[i] == 0 then
 		
 			if bulletx[i] < 2 then
 				bulletx[i] = 2
 			end
 			print(slidestrings[i],
 			bulletx[i],ypos,7)
 		end
		end
 	ypos+=10
 end
	
	--little hint we're about
	--to change slide
	if slidecounter ==
		#slidestrings then
		print("üòê",120,122,7)
	end
	
	--slide number 
	print((slide + 1),1,122,5)
	xoffset = 5
	if (slide + 1) > 9 then

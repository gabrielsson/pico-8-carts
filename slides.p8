	x=time()*8%150-9
	y=14+sin(x/8)*2.5
	line(x,14,x-2,y,2)
	line(x,14,x+2,y)
	rectfill(0,50,127,127)
	for y=0,77 do
	z=77/(y+1)
	for i=0,z*8 do
	x=rnd(160)-16
	w=sin(rnd()+time())*12/z
	if(w>0)line(x-w,y+50,x+w,y+50,max(2,pget(x,49-y/2)))
	end
	end
	flip()
end

